{
  "name": "<%= packageName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist --minified",
    "build:web": "npm run build && browserify dist/index.js > __index.web.js && rimraf dist && mkdir dist && mv __index.web.js dist/index.js",
    "start": "node dist/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "jest .",
    "test:coverage": "jest --coverage .",
    "validate": "yarn test && yarn lint"
  },
  "keywords": ["create-ecma-app", "generated"],
  "author": "<%= author %>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/plugin-transform-modules-umd": "^7.8.3",
    "browserify": "^16.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.1"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "engines": {
    "node": ">= 8.0.0"
  }
}
